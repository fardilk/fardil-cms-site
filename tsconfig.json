{
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "esModuleInterop": true,
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "bundler",
  "noEmit": true,
  "types": ["react", "react-dom", "vite/client"],
  "lib": ["ESNext", "DOM"],
  "skipLibCheck": true
  }
}